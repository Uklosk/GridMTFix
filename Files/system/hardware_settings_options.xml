<hardware_config_options>
    
    <!-- Audio Defaults - This category offers default audio settings which can be loaded to match hardware quality when the game is first run. -->
    <category name="sfx_quality">
        <option name="low" >
            <audio_card>
                <audio reverbs="1" voices="119" />
            </audio_card>
        </option>
        <option name="medium" >
            <audio_card>
                <audio reverbs="1" voices="120" />
            </audio_card>
        </option>
        <option name="high" >
            <audio_card>
                <audio reverbs="1" voices="121" type="default" />
            </audio_card>
        </option>
    </category>   
     
    <category name="sfx_accel" >
      <option name="off" >
        <audio_card>
          <audio mixing="software" />
        </audio_card>
      </option>
      <option name="OpenAL" >
        <audio_card>
          <audio mixing="default" />
        </audio_card>
      </option>
    </category>

